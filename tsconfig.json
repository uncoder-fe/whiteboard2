{
    "compilerOptions": {
        "jsx": "react",
        "experimentalDecorators": true,
        "module": "ES2015",
        "target": "es3",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true
        // "lib": ["es5", "dom"]
    },
    "include": ["src/*.tsx", "src/*.ts"],
    "exclude": ["node_modules"]
}
